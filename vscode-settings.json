{
    "clang-format.executable": "/opt/homebrew/bin/clang-format",
    "editor.formatOnSave": true,
    "workbench.statusBar.visible": true,
    "workbench.sideBar.location": "left",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/*.wk": true,
        "**/*.log": true,
        "**/*.o": true,
        ".cache": true
    },
    "git.enabled": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "build/": true,
        "temp/": true,
        "library/": true,
        "**/*.anim": true
    },
    "workbench.editor.enablePreview": false,
    "files.autoGuessEncoding": true,
    "java.import.generatesMetadataFilesAtProjectRoot": true,
    "python.jediEnabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.associations": {
        "*.txt": "makefile"
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "go.gocodeAutoBuild": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/out*/**": true
    },
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#183d20",
        "statusBar.background": "#c4610b",
        "statusBar.foreground": "#101110"
    },
    "editor.tokenColorCustomizations": {
        "[Shades of Purple]": {
            "textMateRules": [
                {
                    "scope": "storage.type.built-in.primitive.cpp",
                    "settings": {
                        "foreground": "#eaee0f"
                    }
                },
                {
                    "scope": "entity.name.type.parameter.cpp",
                    "settings": {
                        "foreground": "#00fc15"
                    }
                },
                {
                    "scope": "entity.name.function.call.cpp",
                    "settings": {
                        "foreground": "#ff6f00"
                    }
                },
                {
                    "scope": "meta.body.function.definition.cpp",
                    "settings": {
                        "foreground": "#00ff2a"
                    }
                },
                {
                    "scope": "source.cpp",
                    "settings": {
                        "foreground": "#70ee65"
                    }
                }
            ]
        }
    },
    "cmake.configureOnOpen": false,
    "[python]": {
        "editor.formatOnType": true
    },
    "workbench.startupEditor": "welcomePage",
    "tabnine.experimentalAutoImports": true,
    "git.ignoreLegacyWarning": true,
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.showStartPage": false,
    "extensions.ignoreRecommendations": true,
    "editor.accessibilitySupport": "off",
    "gitlens.advanced.messages": {
        "suppressGitDisabledWarning": true,
        "suppressGitVersionWarning": true
    },
